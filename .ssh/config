IdentityFile ~/.ssh/id_rsa

Host github.com
    IdentitiesOnly yes
    IdentityFile ~/.ssh/github_rsa

Host ssh-roc.inria.fr
    HostName ssh-roc.inria.fr
    ProxyCommand none

Host *.inria.fr
    ProxyCommand ssh -q ssh-roc.inria.fr /usr/bin/nc %h %p
    User qpradet
    ForwardAgent yes
    IdentityFile ~/.ssh/inria_rsa

Host bitbucket.org
    IdentitiesOnly yes
    IdentityFile ~/.ssh/bitbucket_rsa

